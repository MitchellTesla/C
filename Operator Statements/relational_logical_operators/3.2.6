   using std::cin;

   int i, i2;
   cout << "Enter the score for the Mets: ";
   cin >> i;

   cout << "\nEnter the score for the Yankees: ";
   cin >> i2;

   if (i > i2)
      cout << "Go i!\n";

   if (i < i2)
   {
      cout << "Go i2!\n";
   }

   if (i == i2)
   {
      cout << "A tie?\n";
