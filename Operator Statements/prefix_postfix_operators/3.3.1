#include <iostream>
 
 int main()
 {
     int intValue = 39;      // initialize two integers
     int yourAge = 39;
     std::cout << intValue << "\n";
     std::cout << yourAge << "\n";
     intValue++;         // postfix increment
     ++yourAge;       // prefix increment
     std::cout << intValue << "\n";
     std::cout << yourAge << "\n";
     std::cout << intValue++ << "\n";
     std::cout << ++yourAge << "\n";
     std::cout << intValue << "\n";
     std::cout << yourAge << "\n";
     return 0;
 }
