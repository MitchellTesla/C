#include <iostream> 
using namespace std; 
 
namespace MyNameSpace { 
  int start; 
  int end; 
 
  class counter { 
     int count; 
   public: 
     counter(int n) {  
       if(n <= start) count = n; 
       else count = start; 
     } 
 
     void reset(int n) { 
       if(n <= start) count = n; 
     } 
 
     int run() { 
       if(count > end) return count--; 
       else return end; 
     } 
  }; 
} 
 
int main() 
{ 
  MyNameSpace::start = 100; 
  MyNameSpace::end = 0; 
 
  MyNameSpace::counter ob1(10); 
  int i; 
 
  do { 
    i = ob1.run(); 
    cout << i << " "; 
  } while(i > MyNameSpace::end); 
  cout << endl; 
 
  MyNameSpace::counter ob2(20); 
 
  do { 
    i = ob2.run(); 
    cout << i << " "; 
  } while(i > MyNameSpace::end); 
  cout << endl; 
 
  ob2.reset(100); 
  MyNameSpace::end = 90; 
  do { 
    i = ob2.run(); 
    cout << i << " "; 
  } while(i > MyNameSpace::end); 
 
  return 0; 
}
