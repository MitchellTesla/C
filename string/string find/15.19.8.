#include <iostream>
using std::cout;
using std::endl;

#include <string>
using std::string;

int main()
{
   string string1( "This is a test string!");
   int location;
   
   location = string1.find_first_not_of( "is" );
   cout << "\n\n(find_first_not_of) '" << string1[ location ]
      << " " << location;

   return 0;
}
